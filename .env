# Web server
PORT=3000
NODE_ENV=production

# Where the server talks to Ollama.
# In Docker, it will be http://ollama:11434
# For local/dev (no Docker), set this to http://localhost:11434
OLLAMA_BASE_URL=http://ollama:11434

# Default model (used by the UI and example pulls)
DEFAULT_MODEL=gemma3:1b
